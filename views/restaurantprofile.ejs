
<div class = 'restaurantprofile'>
<%  restaurant.forEach(function(data){ %>
  <h2><%- data.restaurant_name %></a></h2>
  <b>Serving:</b> <%- data.cuisine %> Cuisine
  <br /> <br />
  <b>In:</b> <%- data.location %>
  <br><br />
</div>

<div class = 'description'>
  <b>Description:</b>
    <%- data.description %>
<br /><br />
  <b>Find them at:</b>
    <%- data.address %>
<br /><br />
  <b>Ongoing Promotions:</b>
  <%- data.promotion %>
</div>

<a href = '/reviews/<%- data.id %>'>Write a Review</a>
<% }) %>

<div class = 'review'>
  <b>Reviews</b>
  <br /><br />

<b>Title: </b><%= review.title %>
<br />
<b>Comment: </b><%= review.comment %>
</div>
